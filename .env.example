# Copy to .env and fill in your provider keys
# At least one provider key is required for LLM answers.

# Groq LLM key (optional)
GROQ_API_KEY=

# OpenAI LLM key (optional)
OPENAI_API_KEY=

# Anthropic LLM key (optional)
ANTHROPIC_API_KEY=

# Google Generative AI key (optional)
GOOGLE_API_KEY=

# Optional: GitHub personal access token for higher rate limits
GITHUB_TOKEN=

# Optional: LangSmith or LangChain API key if enabling LangChain tracing
LANGCHAIN_API_KEY=

# Optional: Enable LangChain-based retrieval (set to true to enable)\nSAGEVAULT_USE_LANGCHAIN=\n
